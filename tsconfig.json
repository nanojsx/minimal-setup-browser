{
  "compilerOptions": {
    "outDir": "dist",

    "target": "ES2015",
    "module": "Node16",
    "moduleResolution": "Node16",

    "jsx": "react-jsx",
    "jsxImportSource": "nano-jsx/lib",

    "allowJs": true,
    "checkJs": true,

    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
